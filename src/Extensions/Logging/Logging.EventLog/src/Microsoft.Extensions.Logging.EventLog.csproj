<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Windows Event Log logger provider implementation for Microsoft.Extensions.Logging.</Description>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <PackageTags>$(PackageTags);eventlog;windowseventlog</PackageTags>
    <IsPackable>true</IsPackable>
    <IsShipping>true</IsShipping>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Diagnostics.EventLog" Version="4.6.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Logging\src\Microsoft.Extensions.Logging.csproj" />
  </ItemGroup>

</Project>
